#include<stdio.h>
void main()
{
   int NUM,i,j,SUM=O;
   clrscr();
   printf("\nENTER INTEGER NUMBER:");
   scanf("%d",&NUM);
   for(i=1;i<NUM+1;i++)
   { 
    if(i%2!=0)
    {
    SUM=SUM+(i*i);
    }
   }
   printf("\nSum of square of odd numbers till %d is:%d",NUM,SUM);
   getch();
}






#include<stdio.h>
#include<conio.h>
void main()
{
	int a[5],max,min,maxpos,minpos,i,temp;
	printf("Enter 5 integers: ");
	for(i=0;i<5;i++)
		scanf("%d",&a[i]);
	max=a[0];
	min=a[0];
	maxpos=0;
	minpos=0;
	for(i=1;i<5;i++)
	{
		if(a[i]>max)
		{
			max=a[i];
			maxpos=i;
		}
		if(a[i]<min)
		{
			min=a[i];
			minpos=i;
		}
	}
	temp=a[maxpos];
	a[maxpos]=a[minpos];
	a[minpos]=temp;
	printf("After interchange array elemnts are: ");
	for(i=0;i<5;i++)
  {
		printf("%d ",a[i]);
  }
	getch();
}








#include<stdio.h>
void main()
{
    float a[100],sum=0,avg;
    int i,n;
    clrscr();
    printf("No. of elements to be entered:");
    scanf("%d",&n);
    printf("Enter the terms:\n");
    for(i=0;i<n;i++)
    { 
    scanf("%f",&a[i]);
    }
    for(i=0;i<n;i++)
    {
      sum=sum+a[i];
    }
    avg=sum/n;
    printf("Average is %f",avg);
    getch();
}



